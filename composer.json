{
    "name": "webthink/guzzle-jwt",
    "homepage": "https://github.com/webthinkgr/guzzle-jwt-middleware",
    "description": "This package contains useful classes and interfaces for requiring a JWT token",
    "keywords": ["webthink", "jwt", "guzzle", "http", "middleware"],
    "license": "MIT",
    "authors": [
        {
            "name": "George Mponos",
            "email": "gmponos@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "symfony/cache": "3.* | 4.*",
        "symfony/http-foundation": "3.* | 4.*",
        "webthink/codesniffer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Webthink\\GuzzleJwt\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webthink\\GuzzleJwt\\Test\\": "tests"
        }
    },
    "scripts": {
        "phpcbf": "php ./vendor/bin/phpcbf src/ tests/",
        "phpcs": "php ./vendor/bin/phpcs src/ tests/",
        "test": "php ./vendor/bin/phpunit --colors=always"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5-dev"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
